<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - bmerketo</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-KK94CHFLLe+nY2dmCWGMq91rCGa5gtU4mk92HdvYe+M/SXH301p5ILy+dN9+nJOZ" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/style.min.css" asp-append-version="true" />

</head>
<body>
    <partial name ="/Views/Partials/_Header.cshtml" />
    @RenderBody()
    <partial name="/Views/Partials/_Footer.cshtml" />


    <script src="https://kit.fontawesome.com/41c8b474e2.js" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ENjdO4Dr2bkBIFxQpeoTz1HIcje39Wm4jDKdf19U8gI4ddQ3GYNS7NTKfAdVQSZe" crossorigin="anonymous"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/validation/email&textarea.js" asp-append-version="true"></script>
    <script src="~/js/validation/firstname.js" asp-append-version="true"></script>
    <script src="~/js/validation/lastname.js" asp-append-version="true"></script>
    <script src="~/js/validation/streetname.js" asp-append-version="true"></script>
    <script src="~/js/validation/postalcode.js" asp-append-version="true"></script>
    <script src="~/js/validation/city.js" asp-append-version="true"></script>
    <script src="~/js/validation/password.js" asp-append-version="true"></script>
    <script src="~/js/validation/name.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>